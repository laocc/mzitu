{
  "name": "lottery 1.0",
  "title": "lottery: composer dump-autoload --optimize",
  "require": {
    "php": ">7.0"
  },
  "require-dev": {
    "php": ">7.0"
  },
  "repositories": [
    {
      "type": "git",
      "url": "https://gitee.com/fazo/IDE_Help.git"
    }
  ],
  "autoload": {
    "files": [
      "esp/helper/Functions.php",
      "helper/functions.php",
      "helper/Bootstrap.php"
    ],
    "psr-4": {
      "esp\\core\\": "esp/core/",
      "esp\\plugs\\": "esp/plugs/",
      "esp\\library\\": "esp/library/",
      "library\\": "library/",
      "resource\\": "resource/",
      "models\\": "models/"
    }
  }
}
